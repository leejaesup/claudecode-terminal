# ClaudeCode Terminal - Environment Variables
# These override config.toml values

# Required
CLAUDECODE_BOT_TOKEN=your_telegram_bot_token_here
CLAUDECODE_ALLOWED_USERS=123456789,987654321

# Optional
# CLAUDECODE_DEFAULT_PROJECT=~/projects
# CLAUDECODE_DEFAULT_MODEL=sonnet
# CLAUDECODE_TIMEOUT=300
# CLAUDECODE_SHELL_TIMEOUT=30
# CLAUDECODE_MAX_OUTPUT=4096
# CLAUDECODE_DB_PATH=~/.claudecode-terminal/history.db
# CLAUDECODE_LOG_LEVEL=WARNING
